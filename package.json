{
  "name": "@luxui/core",
  "version": "0.1.0",
  "description": "Admin UI as a Siren hypermedia client.",
  "main": "src/index.js",
  "scripts": {
    "doc": "jsdoc -c ./jsdoc.conf.json",
    "predoc": "rm -rf ./docs",
    "test": "jest --watch",
    "test:cover": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/luxui/core.git"
  },
  "keywords": [
    "hypermedia",
    "javascript",
    "js",
    "siren",
    "client"
  ],
  "author": "Joshua T Kalis <JoshKalis@QickenLoans.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luxui/core/issues"
  },
  "homepage": "https://github.com/luxui/core#readme",
  "devDependencies": {
    "jest": "^17.0.3",
    "jest-cli": "^17.0.3",
    "jest-fetch-mock": "^1.0.6",
    "jsdoc": "^3.4.3"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "url-parse": "^1.1.7",
    "whatwg-fetch": "^2.0.1"
  },
  "jest": {
    "automock": false,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "./jestInit.js"
    ]
  }
}
