{
  "opts": {
    "destination": "./docs/",
    "readme": "./README.md",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [ "./src/" ],
    "includePattern": ".+\\.js$",
    "excludePattern": ".+\\.test\\.js$"
  },
  "tags": {
    "dictionaries": [ "jsdoc", "closure" ]
  },
  "templates": {
    "cleverLinks": false,
    "default": {
      "includeDate": false
    },
    "monospaceLinks": false
  }
}
